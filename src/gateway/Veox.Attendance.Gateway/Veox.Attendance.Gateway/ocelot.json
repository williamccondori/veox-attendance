{
  "Routes": [
    {
      "DownstreamPathTemplate": "/user",
      "DownstreamScheme": "http",
      "ServiceName": "attendance-user",
      "UpstreamPathTemplate": "/api/users",
      "UpstreamHttpMethod": ["Get"]
    },
    {
      "DownstreamPathTemplate": "/auth/login",
      "DownstreamScheme": "http",
      "ServiceName": "attendance-identity",
      "UpstreamPathTemplate": "/api/auth/login",
      "UpstreamHttpMethod": ["Get", "Post"]
    },
    {
      "DownstreamPathTemplate": "/user",
      "DownstreamScheme": "http",
      "ServiceName": "attendance-identity",
      "UpstreamPathTemplate": "/api/auth/users",
      "UpstreamHttpMethod": ["Get", "Post"]
    },
    {
      "DownstreamPathTemplate": "/workspace/{everything}",
      "DownstreamScheme": "http",
      "ServiceName": "attendance-workspace",
      "UpstreamPathTemplate": "/api/workspaces/{everything}",
      "UpstreamHttpMethod": ["Get", "Post", "Post"]
    }
  ],
  "GlobalConfiguration": {
    "ServiceDiscoveryProvider": {
      "Host": "attendance-service-discovery",
      "Port": 8500
    }
  }
}
